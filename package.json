{
  "name": "imginx",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "crate:build": "wasm-pack build ./crate --target web",
    "crate:watch": "cargo watch -i .gitignore -i \"pkg/*\" -C \"./crate\" -s \"wasm-pack build --target web\""
  },
  "dependencies": {
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.2.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-wasm-pack": "^0.1.12",
    "vue-tsc": "^1.0.9"
  }
}
